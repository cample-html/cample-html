{
  "compilerOptions": {
    "target": "ESNext",
    "noEmit": true,
    "declaration": true,
    "pretty": true,
    "noImplicitAny": false,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": ["./src/cample-html.js"],
  "exclude": ["node_modules"]
}
